{
  "version": 2,
  "public": true,
  "name": "cuentoneta",
  "installCommand": "pnpm install --ignore-scripts",
  "devCommand": "node -r ts-node/register --env-file=.env dist/cuentoneta/server/server.mjs",
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.mjs": {
      "includeFiles": "dist/cuentoneta/**"
    }
  }
}
